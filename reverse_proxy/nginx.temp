events {

}

http {
  server {
    listen 443;
    # ssl_certificate /etc/ssl/pong/selfsigned.csr;
    # ssl_certificate_key /etc/ssl/pong/selfsigned.key;

    location /socket.io/ {
      # proxy_pass http://e2r8p2.clusters.42paris.fr:3011;
      proxy_pass ${REACT_APP_RP_BACK_URL};

      proxy_http_version 1.1;
      proxy_set_header Upgrade $http_upgrade;
      proxy_set_header Connection "upgrade";
      # proxy_pass http://backend:3011;
      # proxy_pass https://localhost:3011/socket.io/;
    }

    location /auth/ {
      # proxy_pass http://e2r8p2.clusters.42paris.fr:3011;
      proxy_pass ${REACT_APP_RP_BACK_URL};
      # proxy_pass http://backend:3011;
      # proxy_pass https://localhost:3011;
    }

    location /match/ {
      # proxy_pass http://e2r8p2.clusters.42paris.fr:3011;
      proxy_pass ${REACT_APP_RP_BACK_URL};
      # proxy_pass http://backend:3011;
      # proxy_pass https://localhost:3011;
    }

    location /user/ {
      # proxy_pass http://e2r8p2.clusters.42paris.fr:3011;
      proxy_pass ${REACT_APP_RP_BACK_URL};
      # proxy_pass http://backend:3011;
      # proxy_pass https://localhost:3011;
    }

    location /api/ {
      proxy_pass ${REACT_APP_RP_BACK_URL};
      # proxy_pass http://e2r8p2.clusters.42paris.fr:3011;
      # proxy_pass http://backend:3011;
      # proxy_pass https://localhost:3011;
    }

    location / {
      proxy_pass ${REACT_APP_RP_FRONT_URL};
      # proxy_pass http://e2r8p2.clusters.42paris.fr:3000;
      # proxy_pass http://frontend:3000;
      # proxy_pass https://localhost:3000;
    }
  }
}